// RUN: sorac -parse-only -verify -dump-parse %s | FileCheck %s

func errors() {
  3
  /3    // expect-error: operator '/' cannot appear at the start of a line
  (3*)  // expect-error: expected an expression after '*'
  3==   // expect-error: expected an expression after '=='
}

func correct() {
  // CHECK: BinaryExpr type=<null_type> + (Add) opLoc=line:[[@LINE+3]]:4
  // CHECK-NEXT: IntegerLiteralExpr type=<null_type> loc=line:[[@LINE+2]]:3 str='3'
  // CHECK-NEXT: IntegerLiteralExpr type=<null_type> loc=line:[[@LINE+1]]:5 str='3'
  3+3
  
  // CHECK: BinaryExpr type=<null_type> + (Add) opLoc=line:[[@LINE+3]]:4
  // CHECK-NEXT: IntegerLiteralExpr type=<null_type> loc=line:[[@LINE+2]]:3 str='3'
  // CHECK-NEXT: IntegerLiteralExpr type=<null_type> loc=line:[[@LINE+2]]:3 str='3'
  3+
  3
}
