  // CHECK: ParamDecl type=i8 identifier=x3 identifierLoc=line:11:3
  x3:   i8,
  // CHECK: ParamDecl type=i16 identifier=x4 identifierLoc=line:13:3
  x4:   i16,
  // CHECK: ParamDecl type=i32 identifier=x5 identifierLoc=line:15:3
  x5:   i32,
  // CHECK: ParamDecl type=i64 identifier=x6 identifierLoc=line:17:3
  x6:   i64,
  // CHECK: ParamDecl type=isize identifier=x7 identifierLoc=line:19:3
  x7:   isize,
  // CHECK: ParamDecl type=u8 identifier=x8 identifierLoc=line:21:3
  x8:   u8,
  // CHECK: ParamDecl type=u16 identifier=x9 identifierLoc=line:23:3
  x9:   u16,
  // CHECK: ParamDecl type=u32 identifier=x10 identifierLoc=line:25:3
  x10:  u32,
  // CHECK: ParamDecl type=u64 identifier=x11 identifierLoc=line:27:3
  x11:  u64,
