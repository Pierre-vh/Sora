  c2 : &(bool, bool, bool),
  // CHECK: ParamDecl type=&mut bool identifier=c3 identifierLoc=line:39:3
  c3 : &mut bool,
  // CHECK: ParamDecl type=maybe bool identifier=c4 identifierLoc=line:41:3
  c4 : maybe bool
) {}