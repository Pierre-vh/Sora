// RUN: %sorac-typecheck -dump-ast %s | %filecheck %s

func foo() {
  // CHECK: TupleExpr type=() numElements=0 lParenLoc=line:[[@LINE+1]]:3 rParenLoc=line:[[@LINE+1]]:4
  ()
  // CHECK: ParenExpr type=i32 lParenLoc=line:[[@LINE+1]]:3 rParenLoc=line:[[@LINE+1]]:5
  (0)
  // CHECK: TupleExpr type=(i32, i32) numElements=2 lParenLoc=line:[[@LINE+1]]:3 rParenLoc=line:[[@LINE+1]]:8
  (0, 1)
  let mut a = 0
  // CHECK: ParenExpr type=@lvalue i32 lParenLoc=line:[[@LINE+1]]:3 rParenLoc=line:[[@LINE+1]]:5
  (a)
  // CHECK: TupleExpr type=@lvalue (@lvalue i32, @lvalue i32) numElements=2 lParenLoc=line:[[@LINE+1]]:3 rParenLoc=line:[[@LINE+1]]:8
  (a, a)
  // CHECK: TupleExpr type=(@lvalue i32, i32, @lvalue i32) numElements=3 lParenLoc=line:[[@LINE+1]]:3 rParenLoc=line:[[@LINE+1]]:11
  (a, 0, a)
}