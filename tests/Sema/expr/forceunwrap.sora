// RUN: sorac -sema-only -verify -dump-ast %s | FileCheck %s

func foo(a: maybe u16, b: maybe ()) {
   0!   // expect-error: cannot force unwrap value of type 'i32' - value must have a 'maybe' type
   // CHECK: ForceUnwrapExpr type=u16 exclaimLoc=line:[[@LINE+1]]:5
   a!
   // CHECK: ForceUnwrapExpr type=() exclaimLoc=line:[[@LINE+1]]:5
   b!   
   c!   // expect-error: cannot find value 'c' in this scope
   let x: maybe ()
   // CHECK: ForceUnwrapExpr type=() exclaimLoc=line:[[@LINE+1]]:5
   x!
}
